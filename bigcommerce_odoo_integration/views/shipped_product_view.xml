<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="bigcommerce_order_line_form_view" model="ir.ui.view">
            <field name="name">Bigcommerce Order Line</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/notebook/page[@name='order_lines']/field[@name='order_line']/tree/field[@name='product_uom_qty']" position="before">
                    <field name="quantity_shipped"/>
                </xpath>
                <xpath expr="//form/sheet/notebook/page[@name='order_lines']/field[@name='order_line']/tree/field[@name='name']" position="before">
                    <field name="x_studio_manufacturer" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>
